{
  "manifest_version": 3,
  "name": "ChatGPT Tab UI",
  "version": "1.0",
  "description": "Customizes OpenAI Chat UI with tabs for each user question.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
